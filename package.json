{
  "name": "fastshare",
  "displayName": "fastshare",
  "description": "Simplifica la conexi贸n en Visual Studio Code Live Share dentro de tu red local.",
  "version": "1.0.5",
  "icon": "images/logoForVSCode.png",
  "engines": {
    "vscode": "^1.86.0"
  },
  "publisher": "GabrielFunes100201",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Gabriel100201/vscode-extension"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fastshare.openServer",
        "title": "openServer"
      },
      {
        "command": "fastshare.openConnection",
        "title": "openConnection"
      },
      {
        "command": "fastshare.showUserList",
        "title": "Actualizar Usuarios",
        "icon": "$(refresh)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "fastshare.showUserList",
          "group": "navigation",
          "when": "view == treeUsers"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "fastshare",
          "title": "fastshare",
          "icon": "images/LogoSVG.svg"
        }
      ]
    },
    "views": {
      "fastshare": [
        {
          "id": "conections",
          "name": "Conexiones"
        },
        {
          "id": "treeUsers",
          "name": "Users List"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "conections",
        "contents": "[Abrir Conexi贸n](command:fastshare.openServer)",
        "when": "openConnectionStatus == 'true'",
        "enablement": "true"
      },
      {
        "view": "conections",
        "contents": "[Conexion Abierta](command:fastshare.openServer)",
        "when": "openConnectionStatus == 'false'",
        "enablement": "false"
      },
      {
        "view": "conections",
        "contents": "[Cargando...](command:fastshare.openServer)",
        "when": "openConnectionStatus == 'loading'",
        "enablement": "false"
      },
      {
        "view": "conections",
        "contents": "[Cerrar Conexi贸n](command:fastshare.closeServer)",
        "when": "openConnectionStatus == 'true'",
        "enablement": "false"
      },
      {
        "view": "conections",
        "contents": "[Cargando...](command:fastshare.closeServer)",
        "when": "openConnectionStatus == 'loading'",
        "enablement": "false"
      },
      {
        "view": "conections",
        "contents": "[Cerrar Conexi贸n](command:fastshare.closeServer)",
        "when": "openConnectionStatus == 'false'",
        "enablement": "true"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --sourcemap",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=./out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --minify",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/bonjour": "^3.5.13",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "bonjour": "^3.5.0",
    "esbuild": "^0.20.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vsls": "^1.0.4753"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "bonjour": "^3.5.0",
    "vsls": "^1.0.4753",
    "ws": "^8.16.0"
  }
}
