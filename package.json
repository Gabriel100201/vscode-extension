{
  "name": "sugerencias",
  "displayName": "sugerencias",
  "description": "extencion para realizar sugerencias",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sugerencias.twComplete",
        "title": "twComplete"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "testbar",
          "title": "Test Bar",
          "icon": "./src/assets/duck.svg"
        }
      ]
    },
    "views": {
      "testbar": [
        {
          "id": "package-dependencies",
          "name": "Barra de prueba"
        },
        {
          "id": "package-outline",
          "name": "Barra de prueba"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "package-dependencies",
        "contents": "[Enviar MSG](command:sugerencias.twComplete)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}
